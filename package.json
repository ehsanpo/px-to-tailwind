{
  "name": "px-to-tailwind",
  "displayName": "px-to-tailwind",
  "description": "Converts px to Tailwind CSS spacing values.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ehsanpo/px-to-tailwind"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pxToTailwind",
          "title": "Px to Tailwind",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "pxToTailwind": [
        {
          "type": "webview",
          "id": "pxToTailwind.sidebarView",
          "name": "Converter"
        }
      ]
    },
    "commands": [
      {
        "command": "px-to-tailwind.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
